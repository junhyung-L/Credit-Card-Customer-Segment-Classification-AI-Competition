   "source": [
    "# ------------------ 재현성 설정 ------------------\n",
    "def seed_everything(seed=42):\n",
    "    random.seed(seed)\n",
    "    np.random.seed(seed)\n",
    "    torch.manual_seed(seed)\n",
    "    torch.cuda.manual_seed(seed)\n",
    "    os.environ['PYTHONHASHSEED'] = str(seed)\n",
    "    torch.backends.cudnn.deterministic = True\n",
    "    torch.backends.cudnn.benchmark = False\n",
    "\n",
    "seed_everything(42)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "id": "a6hNtKoMNjh-"
   },
   "outputs": [],
   "source": [
    "# ------------------ 설정 ------------------\n",
    "USE_TOP_K_FEATURES = True  # 전체 컬럼을 사용할 땐 False로 바꾸기\n",
    "TOP_K = 50  # 사용할 feature 개수 제한\n",
    "CORRELATION_THRESHOLD = 0.05  # 결측치 컬럼 유지 기준"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "id": "hfCnvKwCNnmE"
   },
   "outputs": [],
   "source": [
    "# ------------------ 데이터 로드 및 전처리 ------------------\n",
    "from google.colab import drive\n",
    "drive.mount('/content/drive')\n",
    "train_df=pd.read_csv('/Users/chosunghoon/Downloads/train_sampled_df.csv')\n",
    "eval_df=pd.read_csv('/Users/chosunghoon/Downloads/evaluation_df.csv')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "id": "MZC06CsPNq4G"
   },
   "outputs": [],
   "source": [
    "# 불필요한 컬럼 제거\n",
    "exclude_cols = ['Unnamed: 0', 'ID', '기준년월']\n",
    "train_df = train_df.drop(columns=exclude_cols, errors='ignore')\n",
    "eval_df = eval_df.drop(columns=exclude_cols, errors='ignore')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "id": "8gPvB8j1Nsh1"
   },
   "outputs": [],
   "source": [
    "# 타겟 생성: 회원여부_이용가능 == 0 이면 이탈 고객\n",
    "train_df['target'] = (train_df['회원여부_이용가능'] == 0).astype(int)\n",
    "eval_df['target'] = (eval_df['회원여부_이용가능'] == 0).astype(int)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "id": "To2XftGANunx"
   },
   "outputs": [],
   "source": [
    "# 결측치 100%인 컬럼 처리 (상관관계 높은 컬럼은 채우고, 나머지는 제거)\n",
    "all_null_cols = train_df.columns[train_df.isnull().mean() == 1.0].tolist()\n",
    "to_keep, to_drop = [], []\n",
    "\n",
    "for col in all_null_cols:\n",
    "    if col in train_df.columns:\n",
    "        if train_df[col].dtype in ['float64', 'int64']:\n",
    "            temp_col = train_df[col].fillna(0)\n",
    "            corr = temp_col.corr(train_df['target'])\n",
    "            if abs(corr) >= CORRELATION_THRESHOLD:\n",
    "                to_keep.append(col)\n",
    "            else:\n",
    "                to_drop.append(col)\n",
    "        else:\n",
    "            to_drop.append(col)\n",
    "\n",
    "for col in to_keep:\n",
    "    if train_df[col].dtype in ['float64', 'int64']:\n",
    "        train_df[col] = train_df[col].fillna(0)\n",
    "        eval_df[col] = eval_df[col].fillna(0)\n",
    "    else:\n",
    "        train_df[col] = train_df[col].fillna('Unknown')\n",
    "        eval_df[col] = eval_df[col].fillna('Unknown')\n",
    "\n",
    "train_df.drop(columns=to_drop, inplace=True, errors='ignore')\n",
    "eval_df.drop(columns=to_drop, inplace=True, errors='ignore')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "id": "wkktYAQjNwyU"
   },
   "outputs": [],
   "source": [
    "# 중복 컬럼 제거\n",
    "train_df = train_df.loc[:, ~train_df.T.duplicated()]\n",
    "eval_df = eval_df.loc[:, train_df.columns]  # 평가 데이터는 학습 데이터 컬럼 기준 맞춤"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "id": "WJItbC2yNzOW"
   },
   "outputs": [],
   "source": [
    "# ------------------ feature 선택 ------------------\n",
    "if USE_TOP_K_FEATURES:\n",
    "    from sklearn.feature_selection import mutual_info_classif\n",
    "\n",
    "    X = train_df.drop(columns=['target'])\n",
    "    y = train_df['target']\n",
    "\n",
    "    numeric_X = X.select_dtypes(include=['number'])\n",
    "    top_k_features = mutual_info_classif(numeric_X.fillna(0), y, random_state=42)\n",
    "    top_k_indices = np.argsort(top_k_features)[-TOP_K:]\n",
    "    selected_cols = numeric_X.columns[top_k_indices].tolist()\n",
    "\n",
    "    train_df = train_df[selected_cols + ['target']]\n",
    "    eval_df = eval_df[selected_cols + ['target']]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "id": "aUsYlG6jN1Mp"
   },
   "outputs": [],
   "source": [
    "# ------------------ feature 분리 ------------------\n",
    "continuous_cols = train_df.select_dtypes(include='float64').columns.tolist()\n",
    "categorical_cols = train_df.select_dtypes(include='object').columns.tolist()\n",
    "if 'target' in continuous_cols:\n",
    "    continuous_cols.remove('target')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "id": "cLMHXMqdN21e"
   },
   "outputs": [],
   "source": [
    "# ------------------ NODE 설정 및 학습 ------------------\n",
    "config = TabularConfig(\n",
    "    task=\"classification\",\n",
    "    model_name=\"NODE\",\n",
    "    target=\"target\",\n",
    "    continuous_cols=continuous_cols,\n",
    "    categorical_cols=categorical_cols,\n",
    "    learning_rate=1e-3,\n",
    "    max_epochs=10,\n",
    "    gpus=0\n",
    ")\n",
    "\n",
    "model = TabularModel(config)\n",
    "model.fit(train=train_df, validation=eval_df)\n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "id": "Y0YKu1bhN4ph"
